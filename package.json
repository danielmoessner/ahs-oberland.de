{
  "name": "website",
  "description": "A beautiful and sweet webseite.",
  "license": "UNLICENSED",
  "private": true,
  "version": "0.0.0",
  "browserslist": "defaults",
  "main": "npm run dev",
  "scripts": {
    "build": "gatsby build",
    "dev": "npm-run-all --parallel gatsby cms",
    "gatsby": "gatsby develop",
    "serve": "gatsby serve",
    "kill": "kill-port 8000",
    "cms": "npx netlify-cms-proxy-server",
    "combine": "combine-yaml -i static/admin/base.yml static/admin/collections/*.yml -o static/admin/config.yml",
    "update": "npx npm-check-updates -u && npm install",
    "format": "eslint --fix --ext .js,.jsx src/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.2",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.4",
    "combine-yaml-cli": "0.0.2",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "gatsby": "^4.11.2",
    "gatsby-plugin-eslint": "^4.0.2",
    "gatsby-plugin-image": "^2.11.1",
    "gatsby-plugin-netlify": "^4.2.0",
    "gatsby-plugin-netlify-cms": "^6.11.0",
    "gatsby-plugin-postcss": "^5.11.0",
    "gatsby-plugin-react-helmet": "^5.11.0",
    "gatsby-plugin-scroll-reveal": "0.0.7",
    "gatsby-plugin-sharp": "^4.11.1",
    "gatsby-plugin-sitemap": "^5.11.1",
    "gatsby-remark-images": "^6.11.1",
    "gatsby-source-filesystem": "^4.11.1",
    "gatsby-transformer-remark": "^5.11.1",
    "gatsby-transformer-sharp": "^4.11.0",
    "gatsby-transformer-yaml": "^4.11.0",
    "kill-port": "^1.6.1",
    "netlify-cms-app": "^2.15.70",
    "netlify-cms-locales": "^1.39.0",
    "netlify-cms-proxy-server": "^1.3.23",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.3",
    "prettier": "^2.6.2",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "sanitize-html": "^2.7.0",
    "tailwindcss": "^3.0.23"
  }
